<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huyong.dao.mapper.KindMapper">
    <select id="getKindsByIds" resultType="kindBO">
        select `id`,`kind_name` from `kind`
        where `status` = 0 and id in
        <foreach collection="kindIdList" item="item" open="(" close=")" separator=",">
            #{item}
        </foreach>
    </select>
</mapper>
